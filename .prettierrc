{
	"arrowParens": "always",
		"bracketSpacing": true,
		"endOfLine": "lf",
		"htmlWhitespaceSensitivity": "ignore",
		"insertPragma": false,
		"singleAttributePerLine": false,
		"bracketSameLine": true,
		"jsxSingleQuote": false,
		"printWidth": 100,
		"proseWrap": "preserve",
		"quoteProps": "as-needed",
		"requirePragma": false,
		"semi": true,
		"singleQuote": false,
		"tabWidth": 2,
		"trailingComma": "es5",
		"useTabs": true,
		"embeddedLanguageFormatting": "auto",
		"vueIndentScriptAndStyle": false,
		"plugins": [
			"typescript"
		]
}